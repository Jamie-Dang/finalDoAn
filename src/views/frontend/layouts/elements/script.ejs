<script src="/frontend/javascripts/swiper-bundle.min.js"></script>
<script src="/frontend/javascripts/sweetalert2.all.min.js"></script>
<script>
  const myAlert = (type, message) => {
    Swal.fire({
      position: "center",
      icon: type, // success || error
      title: message,
      showConfirmButton: false,
      timer: 1500,
    });
  };
</script>
